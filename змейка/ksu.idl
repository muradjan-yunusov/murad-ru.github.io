<div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel"
     aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4>Если Ваша учётная запись есть в базе данных, на Ваш адрес электронной почты будет отправлено письмо, содержащее инструкции по восстановлению доступа.</h4>

            </div>
            <div class="modal-body">
                <div class="alert alert-success d-none" role="alert">
                </div>
                <form id="lost-login-request-form" method="post" action="../../../LostLogin" autocomplete="off">
                    <fieldset>
                        <legend>Поиск</legend>
                        <div class="form-group">
                            <input type="text" name="lname" class="form-control"
                                   placeholder="Фамилия">
                        </div>
                        <div class="form-group">
                            <input type="text" name="fname" class="form-control"
                                   placeholder="Имя">
                        </div>
                        <div class="form-group">
                            <input type="text" name="inn" id="iin" class="form-control"
                                   placeholder="ИИН">
                        </div>
                        <div class="form-group">
                            <input type="email" name="email" class="form-control"
                                   placeholder="E-mail">
                            <input type="hidden" name="lang" value="rus">
                            <input type="hidden" name="request_type" value="student_login_request">
                        </div>
                    </fieldset>
                    <button class="btn btn-success">Получить логин и пароль</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Отмена</button>
                </form>
            </div>
        </div>
    </div>
</div>
<script>
    var errorMessages = {
        "ru.first_name_required": "Поле «Имя» является обязательным для заполнения",
        "ru.last_name_required": "Поле «Фамилия» является обязательным для заполнения",
        "ru.iin_required": "Введите корректный ИИН",
        "ru.email_required": "Введите корректный Email",
        "kz.first_name_required": "Толтырылуы керек 'Аты'",
        "kz.last_name_required": "Толтырылуы керек 'Тегі'",
        "kz.iin_required": "Дұрыс ЖСН енгізіңіз",
        "kz.email_required": "Дұрыс электрондық поштаны енгізіңіз",
        "en.first_name_required": "The field is required",
        "en.last_name_required": "The field is required",
        "en.iin_required": "Please enter a valid IIN",
        "en.email_required": "Please enter a valid Email"
    };
    function show(lang) {
        window.location.href = window.location.href.split('?')[0] + '?lang=' + lang;
    }
    function resetLostLoginForm() {
        $("#lost-login-request-form")[0].reset();
        var validator = $("#lost-login-request-form").validate();
        validator.resetForm();
    }
    function getUrlParameter(sParam) {
        var sPageURL = decodeURIComponent(window.location.search.substring(1)),
            sURLVariables = sPageURL.split('&'),
            sParameterName,
            i;

        for (i = 0; i < sURLVariables.length; i++) {
            sParameterName = sURLVariables[i].split('=');

            if (sParameterName[0] === sParam) {
                return sParameterName[1] === undefined ? true : sParameterName[1];
            }
        }
        return "ru";
    }
    $(function(){
        $("#lost-login-request-form").validate({
            rules: {
                lname: "required",
                fname: "required",
                inn: {
                    required: true,
                    number:true,
                    minlength: 12,
                    maxlength: 12
                },
                email: {
                    required: true,
                    email: true
                }
            },
            messages: {
                lname: errorMessages[getUrlParameter("lang") + ".last_name_required"],
                // fname: "Введите имя",
                fname: errorMessages[getUrlParameter("lang") + ".first_name_required"],
                inn: {
                    required: errorMessages[getUrlParameter("lang") + ".iin_required"],
                    minlength: errorMessages[getUrlParameter("lang") + ".iin_required"],
                    maxlength: errorMessages[getUrlParameter("lang") + ".iin_required"],
                    number: errorMessages[getUrlParameter("lang") + ".iin_required"]
                },
                email: errorMessages[getUrlParameter("lang") + ".email_required"]
            }
        });
    });
</script>


</body>
</html>